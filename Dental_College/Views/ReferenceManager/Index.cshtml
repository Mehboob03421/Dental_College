@model IEnumerable<Dental_College.Models.PatientReference>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Reference_Desc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reference_EntryDateTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Reference_Desc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Reference_EntryDateTime)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Reference_Code">Edit</a> |
                @*<a asp-action="Details" asp-route-id="@item.Reference_Code">Details</a>*@ |
